{
  "name": "tsplus",
  "private": true,
  "repository": "https://github.com/ts-plus/stdlib.git",
  "author": "Michael Arnaldi <arnaldimichael@gmail.com>",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "postinstall": "tsplus-install",
    "clean": "ultra -r clean",
    "build": "yarn build-all && yarn build-pack",
    "build-pack": "ultra -r build-pack",
    "build-all": "tsc -b tsconfig.all.json",
    "build-watch": "tsc -b tsconfig.all.json --watch",
    "circular": "ultra -r circular",
    "test": "vitest",
    "autofix": "ultra -r autofix",
    "lint": "ultra -r lint",
    "tc": "ultra -r tc",
    "org:rimraf": "cd $INIT_CWD && rimraf",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:prettier": "cd $INIT_CWD && prettier",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:build-utils": "cd $INIT_CWD && build-utils",
    "org:ts-node": "cd $INIT_CWD && ts-node",
    "org:babel": "cd $INIT_CWD && babel",
    "org:madge": "cd $INIT_CWD && madge"
  },
  "packageManager": "yarn@3.1.1",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@effect-ts/build-utils": "0.38.3",
    "@effect-ts/core": "^0.58.1",
    "@tsplus/installer": "^0.0.37",
    "@types/node": "^17.0.21",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.2",
    "chalk": "5.0.1",
    "concurrently": "^7.0.0",
    "cpx": "^1.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-codegen": "0.16.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "madge": "^5.0.1",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "ultra-runner": "^3.10.5",
    "vite": "^2.7.13",
    "vitest": "https://github.com/mikearnaldi/vitest/raw/f0fd85f779aaefabfcd5a1309bf558e21c9d09f0/packages/vitest/dev/vitest-0.7.6.tgz"
  },
  "resolutions": {
    "eslint-plugin-codegen": "patch:eslint-plugin-codegen@0.16.1#.yarn/patches/eslint-plugin-codegen.patch",
    "@effect-ts/build-utils@0.38.3": "patch:@effect-ts/build-utils@0.38.3#.yarn/patches/effect-ts-build-utils.patch"
  }
}
